{
  "name": "rcs-core",
  "version": "3.5.0",
  "description": "Rename css selectors across all files",
  "main": "dest",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf dest && tsc && babel dest -d dest",
    "lint": "eslint lib/**/*.ts __tests__/**/*.ts",
    "fix": "eslint lib/**/*.ts __tests__/**/*.ts --fix",
    "prepublish": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "css",
    "stylesheet",
    "minify",
    "rename",
    "selectors",
    "shrink",
    "classes",
    "compress",
    "styles",
    "munch"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-rcs-core.git"
  },
  "bugs": {
    "url": "https://github.com/JPeer264/node-rcs-core/issues"
  },
  "homepage": "https://github.com/JPeer264/node-rcs-core#readme",
  "dependencies": {
    "ast-traverse": "^0.1.1",
    "decimal-to-any": "^1.0.5",
    "espree": "^6.1.2",
    "lodash.merge": "^4.6.2",
    "parse5": "^5.1.1",
    "parse5-traverse": "^1.0.3",
    "postcss": "^7.0.26",
    "pug-code-gen": "^2.0.2",
    "pug-lexer": "^4.1.0",
    "pug-parser": "^5.0.1",
    "pug-runtime": "^2.0.5",
    "pug-source-gen": "^0.0.2",
    "pug-walk": "^1.1.8",
    "recast": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@types/jest": "^25.1.2",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^13.7.1",
    "@types/parse5": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "html-minifier": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT"
}
