{
  "name": "rcs-core",
  "version": "2.0.0",
  "description": "Rename css selectors across all files",
  "main": "dest",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "build": "babel lib -d dest",
    "lint": "eslint lib test",
    "fix": "eslint lib test --fix",
    "prepush": "npm test",
    "prepublish": "npm run build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "css",
    "stylesheet",
    "minify",
    "rename",
    "selectors",
    "shrink",
    "classes",
    "compress",
    "styles",
    "munch"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-rcs-core.git"
  },
  "bugs": {
    "url": "https://github.com/JPeer264/node-rcs-core/issues"
  },
  "homepage": "https://github.com/JPeer264/node-rcs-core#readme",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "array-includes": "^3.0.2",
    "ast-traverse": "^0.1.1",
    "decimal-to-any": "1.0.0",
    "espree": "^3.4.3",
    "fs-extra": "^2.0.0",
    "json-extra": "^0.5.0",
    "lodash.merge": "^4.6.1",
    "object.entries": "^1.0.4",
    "postcss": "^6.0.17",
    "recast": "^0.12.5"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "husky": "^0.13.3",
    "nyc": "^11.0.2"
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT"
}
