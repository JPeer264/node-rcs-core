{
  "name": "rcs-core",
  "version": "3.0.0-alpha.3",
  "description": "Rename css selectors across all files",
  "main": "dest",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc && babel dest -d dest",
    "lint": "eslint lib/**/*.ts __tests__",
    "fix": "eslint lib/**/*.ts __tests__ --fix",
    "prepublish": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "css",
    "stylesheet",
    "minify",
    "rename",
    "selectors",
    "shrink",
    "classes",
    "compress",
    "styles",
    "munch"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-rcs-core.git"
  },
  "bugs": {
    "url": "https://github.com/JPeer264/node-rcs-core/issues"
  },
  "homepage": "https://github.com/JPeer264/node-rcs-core#readme",
  "dependencies": {
    "ast-traverse": "^0.1.1",
    "decimal-to-any": "1.0.0",
    "espree": "^5.0.0",
    "fs-extra": "^2.0.0",
    "json-extra": "^0.5.0",
    "lodash.merge": "^4.6.1",
    "parse5": "^5.0.0",
    "parse5-traverse": "^1.0.3",
    "postcss": "^7.0.8",
    "pug-code-gen": "^2.0.1",
    "pug-lexer": "^4.0.0",
    "pug-parser": "^5.0.0",
    "pug-runtime": "^2.0.4",
    "pug-source-gen": "^0.0.2",
    "pug-walk": "^1.1.7",
    "recast": "^0.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@types/jest": "^24.0.18",
    "@types/lodash.merge": "^4.6.6",
    "@types/parse5": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "coveralls": "^2.11.15",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "html-minifier": "^3.5.18",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT"
}
